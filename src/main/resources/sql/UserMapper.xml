<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
 <!-- namespace指定和哪个Mapper映射器接口对应 -->
<mapper namespace="com.ysf.bootdemo.dao.UserDao">
	<!-- 定义SQL语句 -->	
    <insert id="addOne" parameterType="com.ysf.bootdemo.entity.User">
        insert into boot_user(name,password) values (#{name},#{password})
    </insert>
    
</mapper>











